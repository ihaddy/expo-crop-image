{
  "name": "@tahsinz21366/expo-crop-image",
  "version": "0.2.2",
  "description": "Allows user to crop image. No fixed aspect ratio required",
  "main": "dist/index.js",
  "types": "dist/@types/index.d.ts",
  "scripts": {
    "build": "tsc --build && npm run move-types",
    "build:dev": "rm -rf ./example-app/node_modules/expo-crop-image && tsc --build tsconfig.dev.json && npm run move-types:dev",
    "build:watch": "tsc --watch --project tsconfig.dev.json",
    "prepare": "husky install",
    "lint": "eslint src --ext .tsx,.ts",
    "lint:fix": "npm run lint -- --fix",
    "commit": "git-cz",
    "move-types": "mkdir ./dist/@types && cp ./src/@types/index.d.ts ./dist/@types/",
    "move-types:dev": "mkdir ./example-app/node_modules/expo-crop-image/dist/@types && cp ./src/@types/index.d.ts ./example-app/node_modules/expo-crop-image/dist/@types/ && cp package.json ./example-app/node_modules/expo-crop-image/"
  },
  "keywords": [
    "expo-crop-image",
    "edit-image",
    "crop-image",
    "edit",
    "crop",
    "image",
    "image-picker",
    "react-native",
    "expo",
    "ios",
    "android"
  ],
  "author": "tahsinz21366",
  "repository": {
    "type": "git",
    "url": "https://github.com/importTahsinZaman/expo-crop-image"
  },
  "license": "MIT",
  "peerDependencies": {
    "@expo/vector-icons": "*",
    "expo-image-manipulator": "*",
    "react": "*",
    "react-native": "*",
    "react-native-gesture-handler": "*"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@expo/vector-icons": "13.0.0",
    "@rocketseat/eslint-config": "^1.1.3",
    "@types/react": "^18.0.25",
    "@types/react-native": "^0.70.6",
    "commitizen": "^4.2.5",
    "eslint": "^8.29.0",
    "expo-image-manipulator": "^11.0.0",
    "husky": "^8.0.2",
    "prettier": "^2.8.0",
    "react": "^18.1.0",
    "react-native": "^0.70.5",
    "react-native-gesture-handler": "~2.8.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "recoil": "0.7.3"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "src/**": "npm run lint:fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "files": [
    "dist/**"
  ]
}
